<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cardview="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_search_result__medical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.LeelaGroup.AgrawalFedration.medical.SearchResult_Medical">

    <include layout="@layout/toolbar_matrimony">

    </include>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                tools:ignore="UselessParent">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical"
                        android:padding="@dimen/TextInputLayoutMargin_top_bottom"
                        tools:ignore="UselessParent"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="true">

                        <TextView
                            android:id="@+id/heading"
                            android:layout_margin="@dimen/layoutMarginBottom"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="SWASTHYA MEDICAL"
                            android:textAppearance="@style/TextAppearance.AppCompat.Title"
                            android:textColor="#8c0303"
                            android:paddingLeft="@dimen/layoutMargin"
                            />

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="@dimen/image_width"
                            android:layout_height="@dimen/image_height"
                            android:layout_gravity="center"
                            android:id="@+id/img_card"/>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_margin="@dimen/layoutMargin"
                            tools:ignore="UseCompoundDrawables"
                            android:weightSum="1">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/tag_heart"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/textViewDetailHeading"
                                android:layout_width="@dimen/text_Width"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:layout_marginTop="@dimen/marginButton1"
                                android:text="@string/Detail"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />
                             <TextView
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:text=":"
                                 android:textAlignment="center"
                                 android:textStyle="bold"/>

                            <TextView
                                android:id="@+id/textViewDetail"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </LinearLayout>

                        <!--<LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">-->

                          <!--  <TextView
                                android:id="@+id/textViewDetail"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />
-->
                     <!--   </LinearLayout>-->

                        <include layout="@layout/item_divider" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/MarginTopCardField"
                            android:orientation="horizontal"
                            android:layout_margin="@dimen/layoutMargin"
                            tools:ignore="UseCompoundDrawables">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/home_map_marker"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/textViewAddress"
                                android:layout_width="@dimen/text_Width"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:text="@string/Address"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=":"
                                android:textAlignment="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/textViewAdress"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />


                        </LinearLayout>


                        <include layout="@layout/item_divider" />
                        <LinearLayout
                            android:layout_margin="@dimen/layoutMargin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/MarginTopCardField"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/watch"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/textViewTiming"
                                android:layout_width="@dimen/text_Width"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:text="@string/Timing"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=":"
                                android:textAlignment="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/textViewTimin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />
                        </LinearLayout>

                        <include layout="@layout/item_divider" />
                        <LinearLayout
                            android:layout_margin="@dimen/layoutMargin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/MarginTopCardField"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/phone_in_talk"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/textViewPh"
                                android:layout_width="@dimen/text_Width"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:text="@string/Phone"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=":"
                                android:textAlignment="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/textViewContact"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />


                        </LinearLayout>

                        <include layout="@layout/item_divider" />
                        <LinearLayout
                            android:layout_margin="@dimen/layoutMargin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/MarginTopCardField"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/ivWebsite"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_web_black_24dp"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/tv_WebsiteContact"
                                android:layout_width="@dimen/text_Width"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:text="@string/Websites"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=":"
                                android:textAlignment="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/tvWebsite"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </LinearLayout>
                        <include layout="@layout/item_divider" />

                        <LinearLayout
                            android:layout_margin="@dimen/layoutMargin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/MarginTopCardField"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/ivContactPerson"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_person_outline_black_24dp"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/Person"
                                android:layout_width="@dimen/text_Width"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:text="@string/Person"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=":"
                                android:textAlignment="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/tvPersonName"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </LinearLayout>
                        <include layout="@layout/item_divider" />
                        <LinearLayout
                            android:layout_margin="@dimen/layoutMargin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/MarginTopCardField"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/ivContactPersonMob"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_tablet_android_black_24dp"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/PersonMob"
                                android:layout_width="@dimen/text_Width"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:text="@string/Mobile_No"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=":"
                                android:textAlignment="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/tvPersonMob"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </LinearLayout>
                        <include layout="@layout/item_divider" />
                        <LinearLayout
                            android:layout_margin="@dimen/layoutMargin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/MarginTopCardField"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/ivContactDesignation"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/ic_nature_people_black_24dp"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/Contact_Designation"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:text="@string/Designation"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=":"
                                android:textAlignment="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/tvDesignation"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </LinearLayout>
                        <include layout="@layout/item_divider" />
                        <LinearLayout
                            android:layout_margin="@dimen/layoutMargin"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/MarginTopCardField"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/mail_ru"
                                tools:ignore="ContentDescription" />

                            <TextView
                                android:id="@+id/textViewEmail"
                                android:layout_width="@dimen/text_Width"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layoutMarginBottom"
                                android:text="@string/Email"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=":"
                                android:textAlignment="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/textView_Email"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/marginButton1"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </LinearLayout>


                    </LinearLayout>
                </RelativeLayout>


            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
    <!--</LinearLayout>
-->
</LinearLayout>
