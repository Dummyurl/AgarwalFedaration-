<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cardview="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_search_result__medical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.LeelaGroup.AgrawalFedration.medical.SearchResult_Medical">

    <include layout="@layout/toolbar_matrimony">

    </include>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                tools:ignore="UselessParent">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:orientation="vertical"
                        android:padding="@dimen/TextInputLayoutMargin_top_bottom"
                        tools:ignore="UselessParent">



                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/img_card"
                            android:layout_width="@dimen/image_width"
                            android:layout_height="@dimen/image_height"
                            android:layout_gravity="center" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="@dimen/layoutMargin">

                            <TextView
                                android:id="@+id/medical"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/medical_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />

                            <TextView
                                android:id="@+id/heading"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/medical"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />
                        </RelativeLayout>
                        <include layout="@layout/item_divider" />
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin"
                            tools:ignore="UseCompoundDrawables">


                            <TextView
                                android:id="@+id/textViewDetailHeading"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/Detail_ic"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary"
                                android:textSize="18sp" />


                            <TextView
                                android:id="@+id/textViewDetail"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/textViewDetailHeading"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </RelativeLayout>


                        <include layout="@layout/item_divider" />


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin"
                            tools:ignore="UseCompoundDrawables">


                            <TextView
                                android:id="@+id/Address_medical"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/add_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />


                            <TextView
                                android:id="@+id/textViewAdress"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/Address_medical"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />


                        </RelativeLayout>

                        <include layout="@layout/item_divider" />


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin">

                            <TextView
                                android:id="@+id/textViewTiming"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/timeing_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />

                            <TextView
                                android:id="@+id/textViewTimin"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/textViewTiming"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />
                        </RelativeLayout>

                        <include layout="@layout/item_divider" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin">


                            <TextView
                                android:id="@+id/textViewPh"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/phone_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentStart="true" />

                            <TextView
                                android:id="@+id/textViewContact"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/textViewPh"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />


                        </RelativeLayout>

                        <include layout="@layout/item_divider" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin">


                            <TextView
                                android:id="@+id/tv_WebsiteContact"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/website_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />


                            <TextView
                                android:id="@+id/tvWebsite"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/tv_WebsiteContact"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </RelativeLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:id="@+id/hide">
                            <include layout="@layout/item_divider" />
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin">


                            <TextView
                                android:id="@+id/Person"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/person_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />

                            <TextView
                                android:id="@+id/tvPersonName"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/Person"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </RelativeLayout>

                        <include layout="@layout/item_divider" />


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin">


                            <TextView
                                android:id="@+id/PersonMob"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/mobile_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />

                            <TextView
                                android:id="@+id/tvPersonMob"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/PersonMob"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </RelativeLayout>

                        <include layout="@layout/item_divider" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin">


                            <TextView
                                android:id="@+id/Contact_Designation"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/designation_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />

                            <TextView
                                android:id="@+id/tvDesignation"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/Contact_Designation"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </RelativeLayout>

                        <include layout="@layout/item_divider" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/layoutMargin">


                            <TextView
                                android:id="@+id/textViewEmail"
                                android:layout_width="@dimen/radio_button_height"
                                android:layout_height="wrap_content"
                                android:text="@string/email_ic"
                                android:textSize="18sp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColor="@color/colorPrimary" />

                            <TextView
                                android:id="@+id/textView_Email"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@+id/textViewEmail"
                                android:scrollbars="vertical"
                                android:textAppearance="@android:style/TextAppearance.DeviceDefault" />

                        </RelativeLayout>

                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>


            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
    <!--</LinearLayout>
-->
</LinearLayout>
