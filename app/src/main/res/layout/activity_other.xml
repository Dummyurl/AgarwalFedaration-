-->

                    <TextView
                        android:id="@+id/d_per_hobies"
                        android:layout_toEndOf="@+id/per_hobbies"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                    />
                </RelativeLayout>

                <include layout="@layout/item_divider1"/>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/layout_margin"
              >

                    <TextView
                        android:id="@+id/per_interests"
                        android:layout_width="@dimen/cont_us_logo_width"
                        android:layout_height="wrap_content"
                        android:text="@string/thumbsup_icon"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/form_tv_size"
                        android:textStyle="bold" />
<!--
                    <TextView
                        android:layout_width="@dimen/seperator_width"
